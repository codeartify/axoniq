{
  "name": "fitnesslab",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "generate-api": "openapi-generator-cli generate -i ../../backend/src/main/resources/openapi.yaml -g typescript-angular -o src/app/generated-api --additional-properties=ngVersion=21.0.0,npmName=fitnesslab-api,supportsES6=true,withInterfaces=true",
    "generate-api:docker": "openapi-generator-cli generate -i /tmp/openapi.yaml -g typescript-angular -o src/app/generated-api --additional-properties=ngVersion=21.0.0,npmName=fitnesslab-api,supportsES6=true,withInterfaces=true"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "21.0.2",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^8.0.0",
    "@tiptap/core": "^3.15.3",
    "@tiptap/extension-blockquote": "^3.15.3",
    "@tiptap/extension-bullet-list": "^3.15.3",
    "@tiptap/extension-color": "^3.15.3",
    "@tiptap/extension-font-family": "^3.15.3",
    "@tiptap/extension-heading": "^3.15.3",
    "@tiptap/extension-highlight": "^3.15.3",
    "@tiptap/extension-image": "^3.15.3",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-list-item": "^3.15.3",
    "@tiptap/extension-ordered-list": "^3.15.3",
    "@tiptap/extension-table": "^3.15.3",
    "@tiptap/extension-table-cell": "^3.15.3",
    "@tiptap/extension-table-header": "^3.15.3",
    "@tiptap/extension-table-row": "^3.15.3",
    "@tiptap/extension-text-align": "^3.15.3",
    "@tiptap/extension-text-style": "^3.15.3",
    "@tiptap/extension-underline": "^3.15.3",
    "@tiptap/pm": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "angular-oauth2-oidc": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "21.0.2",
    "@openapitools/openapi-generator-cli": "^2.25.2",
    "@tailwindcss/postcss": "^4.1.12",
    "angular-eslint": "21.0.1",
    "eslint": "^9.39.1",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.4",
    "vitest": "^4.0.8"
  }
}
